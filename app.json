{
  "expo": {
    "name": "um-reactnative",
    "slug": "um-reactnative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/img/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "splash": {
        "image": "./app/assets/img/splash/splash.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "org.markup.exp"
    },
    "android": {
      "splash": {
        "mdpi": "./app/assets/img/splash/splash-mdpi.png",
        "hdpi": "./app/assets/img/splash/splash-hdpi.png",
        "xhdpi": "./app/assets/img/splash/splash-xhdpi.png",
        "xxhdpi": "./app/assets/img/splash/splash-xxhdpi.png",
        "resizeMode": "cover"
      },
      "package": "org.markup.exp"
    }
  }
}